default 
{
 RF discharge
}
///  for dt=0.25e-12
///  1 RF cycles is 25000  timesteps
///  2 RF cycles is 50000 timesteps
///  4 RF cycles is 100000 timesteps
Variables
{
	Ia = 900 //current amplitude
	phi1 = PI/2 //phase on electrodes 1,3
	phi2 = 3*PI/2 //phase on electrodes 2,4
	dtk = 0.0078125   //interelectrode dielectric thinckness // 20 cells
	tl  = 0.02421875  //tile lenght  // 62 cells
	//the condition is that 6tl+7dtk = 512
	Itk =0.00078125 //current region thickness
	ed =10 //dielectric constant
	rf=160E6
	cyc = 200 // number of iteration for saving into the diagnostic files
	res_energ =200
	e_max =100

	height = 0.025
		
}
Region
{	
Species
{
        name = electrons
        m = 9.11E-31
        q = -1.6E-19
        collisionModel = 1
	 particleLimit = 2.4e+7
}
Species
{
        name = argon
        m = 6.67e-26
        q = 1.6E-19
        collisionModel = 0
        subcycle = 100
	 particleLimit = 2.4e+7
}
MCC
{
        collisionFlag = 1
        gas = Ar
        pressure = 0.025
        eSpecies = electrons
        iSpecies = argon
}
Grid
{
	Geometry = 1    // XY co-ordinates
	J = 512
	x1s = 0.0
	x1f = 0.2
	n1 = 1.0
	K = 160
	x2s = 0.0
	x2f = height
	n2 = 1.0
}

Control
{
	dt = 0.25E-12
	ElectrostaticFlag = 0
	initPoissonSolve = 1  //  JK Lee: do Poisson solve on restart 
	// emdamping = 0.2
        //  nSmoothing = 5
   //  John Verbonquer  not for E-M
}
Conductor  //   should be     Equipotential for EM 
{       		// Equi:  grounded conductor in EM case.  outter conductor
// name = chamber	//    behind top-layer dielectric, so no plasma exposure
// EFFlag = 1
// IdiagFlag = 1
// nxbins = 512
// nenergybins = res_energ
// energy_max = e_max

//	Secondary      //  ***** adding secondary electrons from edges. 
//	{
//	  secondary = 0.25
//	  secSpecies = electrons
//	  iSpecies = argon
//     }
	
	Segment    // across top 
	{
	
	  A1 = 0
	  B1 = 0.2
	  A2 = height
	  B2 = height
	  normal = -1
	}
//        Segment   //  down RHS
//        {
//          A1 = 0.2
//          B1 = 0.2
//          A2 = height
//          B2 = 0
//         normal = -1
//        }
        Segment    //  across bottom 
        {
          A1 = 0.2
          B1 = 0
          A2 = 0
          B2 = 0
          normal = 1
        }
//	Segment     //   up LHS
//	{
//	  A1 = 0
//	  B1 = 0
//	  A2 = 0
//	  B2 = height
//	  normal = 1
//	}
}

Equipotential  //   should be     Equipotential for EM 
{       		// Equi:  grounded conductor in EM case.  outter conductor

	Secondary      //  ***** adding secondary electrons from edges. 
	{
	  secondary = 0.75
	  secSpecies = electrons
	  iSpecies = argon
      }

      Segment      //   up RHS
        {
          A1 = 0.2
          B1 = 0.2
          A2 = 0
          B2 = height
          normal = -1
        }
	Segment    //   up LHS 
	{
	  A1 = 0
	  B1 = 0
	  A2 = 0
	  B2 = height
	  normal = 1
	}
}



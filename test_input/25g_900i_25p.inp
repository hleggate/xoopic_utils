Conductor   //Tile 1
{
fill = 1
name = Tile_1
EFFlag = 1
IdiagFlag = 1
nxbins = tl*5
nenergybins = res_energ
energy_max = e_max

	
	Segment		//	Top-side
	{
	
	  A1 = dtk
	  A2 = 0.0025
	  B1 = dtk+tl
	  B2 = 0.0025
	  normal = 1
	}
	Segment		//	Down RHS
	{
	  A1 = dtk+tl
	  A2 = 0.0025
	  B1 = dtk+tl
	  B2 = 0.00125
	  normal = 1
	}
	Segment		//	Bottom-side
	{
          A1 = dtk+tl
          A2 = 0.00125
          B1 = dtk
          B2 = 0.00125
          normal = -1
	}
       
	Segment		//	Up LHS
	{
	  A1 = dtk
	  A2 = 0.00125
	  B1 = dtk
	  B2 = 0.0025
	  normal = -1
	}

 Secondary
        {
            secondary = .1
            secSpecies = electrons
            iSpecies = argon
        }
}

